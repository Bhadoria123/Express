
<%- include('../../layout/header.ejs') %>
<section class="dashboard-wrap mtb-40">
    <div class="container">
        <div class="body-content">
            <div class="row">
                <div class="col-md-3">
                    <div class="dash-left">
                        <ul>
                            <li class="active"><a href="/admin/dashboard" class="active">Dashboard</a></li>
                            <li><a href="/admin/blogdisplay">Blogs</a></li>
                            <li><a href="/admin/categorydisplay">Categories</a></li>
                            <li><a href="/admin/contactdisplay">Contact</a></li>
                            <li><a href="/admin/aboutdisplay">About</a></li>
                           
                            <li><a href="/logout">Logout</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="dash-right">
                        <div class="dash-header">
                            <div class="dash-title">
                                <h1>Contact</h1>
                            </div>
                           
                        </div>
                        <table class="table table-bordered table-striped">
                            <tr>
                                <th>Id</th>
                                <th>Name</th>

                                <th>Email</th>
                                

                                <th>Phone</th>
                                <th>Message</th>
                            </tr>

                              <% d.forEach((item)=>{ %>
                                    <tr>

                                        <td><%= item._id %></td>
                                        <td><%= item.name %></td>
                                        <td><%= item.email %></td>
                                        <td><%= item.phone %></td>
                                        <td><%= item.message%></td>

                                    </tr>
                                    <% })%>
                            


                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</section>

<%- include('../../layout/footer.ejs') %>
